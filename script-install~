mvn install:install-file -Dfile=./jogl-all-natives-linux-amd64 -DpomFile=./pom-jogl-all-natives-linux-amd64.xml
mvn install:install-file -Dfile=./jogl-all-natives-macosx-universal -DpomFile=./pom-jogl-all-natives-macosx-universal.xml
mvn install:install-file -Dfile=./jogl-all-natives-linux-i586 -DpomFile=./pom-jogl-all-natives-linux-i586.xml
mvn install:install-file -Dfile=./jogl-all-natives-solaris-amd64 -DpomFile=./pom-jogl-all-natives-solaris-amd64.xml
mvn install:install-file -Dfile=./jogl-all-natives-solaris-i586 -DpomFile=./pom-jogl-all-natives-solaris-i586.xml
mvn install:install-file -Dfile=./jogl-all-natives-windows-amd64 -DpomFile=./pom-jogl-all-natives-windows-amd64.xml
mvn install:install-file -Dfile=./jogl-all-natives-windows-i586 -DpomFile=./pom-jogl-all-natives-windows-i586.xml

mvn install:install-file -Dfile=./jocl -DpomFile=./pom-jocl.xml
mvn install:install-file -Dfile=./jocl-natives-linux-amd64 -DpomFile=./pom-jocl-natives-linux-amd64.xml
mvn install:install-file -Dfile=./jocl-natives-macosx-universal -DpomFile=./pom-jocl-natives-macosx-universal.xml
mvn install:install-file -Dfile=./jocl-natives-linux-i586 -DpomFile=./pom-jocl-natives-linux-i586.xml
mvn install:install-file -Dfile=./jocl-natives-solaris-amd64 -DpomFile=./pom-jocl-natives-solaris-amd64.xml
mvn install:install-file -Dfile=./jocl-natives-solaris-i586 -DpomFile=./pom-jocl-natives-solaris-i586.xml
mvn install:install-file -Dfile=./jocl-natives-windows-amd64 -DpomFile=./pom-jocl-natives-windows-amd64.xml
mvn install:install-file -Dfile=./jocl-natives-windows-i586 -DpomFile=./pom-jocl-natives-windows-i586.xml

mvn install:install-file -Dfile=./joal -DpomFile=./pom-joal.xml
mvn install:install-file -Dfile=./joal-natives-linux-amd64 -DpomFile=./pom-joal-natives-linux-amd64.xml
mvn install:install-file -Dfile=./joal-natives-macosx-universal -DpomFile=./pom-joal-natives-macosx-universal.xml
mvn install:install-file -Dfile=./joal-natives-linux-i586 -DpomFile=./pom-joal-natives-linux-i586.xml
mvn install:install-file -Dfile=./joal-natives-solaris-amd64 -DpomFile=./pom-joal-natives-solaris-amd64.xml
mvn install:install-file -Dfile=./joal-natives-solaris-i586 -DpomFile=./pom-joal-natives-solaris-i586.xml
mvn install:install-file -Dfile=./joal-natives-windows-amd64 -DpomFile=./pom-joal-natives-windows-amd64.xml
mvn install:install-file -Dfile=./joal-natives-windows-i586 -DpomFile=./pom-joal-natives-windows-i586.xml

