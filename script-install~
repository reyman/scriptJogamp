#!/bin/bash
mvn install:install-file -Dfile=./gluegen -DpomFile=./pom-gluegen.xml

mvn install:install-file -Dfile=./gluegen-rt -DpomFile=./pom-gluegen-rt.xml
mvn install:install-file -Dfile=./gluegen-rt-natives-linux-amd64 -DpomFile=./pom-gluegen-rt-natives-linux-amd64.xml
mvn install:install-file -Dfile=./gluegen-rt-natives-macosx-universal -DpomFile=./pom-gluegen-rt-natives-macosx-universal.xml
mvn install:install-file -Dfile=./gluegen-rt-natives-linux-i586 -DpomFile=./pom-gluegen-rt-natives-linux-i586.xml
mvn install:install-file -Dfile=./gluegen-rt-natives-solaris-amd64 -DpomFile=./pom-gluegen-rt-natives-solaris-amd64.xml
mvn install:install-file -Dfile=./gluegen-rt-natives-solaris-i586 -DpomFile=./pom-gluegen-rt-natives-solaris-i586.xml
mvn install:install-file -Dfile=./gluegen-rt-natives-windows-amd64 -DpomFile=./pom-gluegen-rt-natives-windows-amd64.xml
